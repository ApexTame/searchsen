{
  "name": "searchsen",
  "displayName": "Searchsen",
  "version": "4.0.0",
  "description": "Browser extension to search and analyze the chat logs of Twitch VODs",
  "author": "Burak Duruk",
  "scripts": {
    "dev": "plasmo dev",
    "build": "pnpm dlx eslint && plasmo build",
    "package": "pnpm dlx eslint && plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "plasmo": "0.89.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.4.0",
    "react-rnd": "^10.4.14",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "3.2.4",
    "prettier-plugin-classnames": "^0.7.5",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "5.3.3",
    "typescript-eslint": "^8.19.1"
  },
  "manifest": {
    "host_permissions": [
      "*://*.twitch.tv/*"
    ],
    "permissions": [
      "tabs",
      "webNavigation"
    ]
  }
}
