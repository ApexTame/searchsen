{
  "name": "searchsen",
  "displayName": "Searchsen",
  "version": "4.0.0",
  "description": "Browser extension to search and analyze the chat logs of Twitch VODs",
  "author": "Burak Duruk",
  "scripts": {
    "dev": "plasmo dev",
    "lint": "eslint . --ext .ts,.tsx",
    "build": "pnpm lint && plasmo build",
    "package": "pnpm lint && plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.13.0",
    "plasmo": "0.89.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.4.0",
    "react-rnd": "^10.4.14",
    "react-virtuoso": "^4.12.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "3.2.4",
    "prettier-plugin-classnames": "^0.7.5",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "*://*.twitch.tv/*"
    ],
    "permissions": [
      "tabs",
      "webNavigation"
    ]
  }
}
